{
  "name": "balonmanovetusta",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "check": "astro check",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "fmt": "prettier . --write",
    "lint": "eslint --no-error-on-unmatched-pattern --no-warn-ignored --cache --fix **/*.{js,jsx,ts,tsx,astro} src",
    "rfebm-scripts": "bun run ./scripts/rfebm-data.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/vercel": "8.0.0-beta.2",
    "@justinribeiro/lite-youtube": "1.5.0",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vercel/og": "0.6.3",
    "@vercel/speed-insights": "1.0.12",
    "ably": "2.4.1",
    "agent-base": "7.1.1",
    "astro": "5.0.0-beta.5",
    "http-proxy-agent": "7.0.2",
    "https-proxy-agent": "7.0.5",
    "ioredis": "5.4.1",
    "pac-proxy-agent": "7.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "socks-proxy-agent": "8.0.4",
    "typescript": "5.6.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bun": "1.1.11",
    "@typescript-eslint/parser": "8.10.0",
    "eslint": "9.13.0",
    "eslint-plugin-astro": "1.3.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "husky": "9.1.6",
    "lightningcss": "1.27.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "ttf2woff2": "6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": "bun lint"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "printWidth": 100
  }
}
