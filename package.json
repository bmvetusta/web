{
  "name": "balonmanovetusta",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "check": "astro check",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "fmt": "prettier . --write",
    "lint": "eslint --no-error-on-unmatched-pattern --no-warn-ignored --cache --fix **/*.{js,jsx,ts,tsx,astro} src",
    "rfebm-scripts": "bun run ./scripts/rfebm-data.ts"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.1.0",
    "@astrojs/vercel": "8.0.0",
    "@astropub/worker": "0.2.0",
    "@justinribeiro/lite-youtube": "1.6.0",
    "@number-flow/react": "0.2.6",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@vercel/og": "0.6.4",
    "@vercel/speed-insights": "1.1.0",
    "ably": "2.6.0",
    "agent-base": "7.1.3",
    "astro": "5.0.5",
    "http-proxy-agent": "7.0.2",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.4.1",
    "pac-proxy-agent": "7.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "socks-proxy-agent": "8.0.5",
    "typescript": "5.7.2",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/bun": "1.1.14",
    "@typescript-eslint/parser": "8.18.0",
    "eslint": "9.16.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "husky": "9.1.7",
    "lightningcss": "1.28.2",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "ttf2woff2": "6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": "bun lint"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "printWidth": 100
  }
}
