{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["*", "src/*"],
      "@schemas/*": ["src/schema/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@public/*": ["public/*"],
      "@well-known/*": ["src/pages/.well-known/*"]
    },
    "lib": ["es2023"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "allowJs": true
  }
}
